doctype
html(lang="es")
  head
    meta(charset="UTF-8")
    title Elementos de Entradas (Formulario)
  body
    h1 Eementos de entradas (Formulario)
    hr
    form(method="post")
      label(for="lenguajes") Lista de Lenguajes de Programacion
      input(list="lenguajes") 
      datalist(id="lenguajes")
        option(value="Python") Python
        option(value="Ruby") Ruby
        option(value="Php") Php
        option(value="C#") C#
        option(value="C++") C++
        option(value="javascript") javascript
      br
      br
      label(for="generador") Generador de Claves
      keygen(id="generador")
      br
      br
      label(for="salida") Salida:
      input(type="range", id="range", oninput="salida.value=this.value")
      output(id="salida") 0
  
  